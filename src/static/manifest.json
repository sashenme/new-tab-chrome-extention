{
  "manifest_version" : 3,
  "name" : "New Tab Chrome extention",
  "description": "New tab with Google Tasks, multiple timezones, weather and your quick links",
  "author": "Sashen Pasindu",
  "version": "1.0.0",
  "chrome_url_overrides":{
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "/img/app_icons/16.png",
    "32": "/img/app_icons/32.png",
    "48": "/img/app_icons/48.png",
    "128": "/img/app_icons/128.png"
},
  "permissions":["geolocation","topSites","storage"]
}